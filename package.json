{
  "name": "arkanoid",
  "version": "0.1.0",
  "description": "Arkanoid",
  "engines": {
    "node": "12.14.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "webpack-dev-server --open",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack -p --mode production",
    "lint": "eslint .",
    "analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "serve": "node server.js",
    "deploy": "source .env && git push -f https://heroku:$HEROKU_API_KEY@git.heroku.com/$HEROKU_APP_NAME.git master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shonie/arkanoid.git"
  },
  "keywords": [
    "arcade"
  ],
  "author": "shonie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shonie/arkanoid/issues"
  },
  "homepage": "https://github.com/shonie/arkanoid#readme",
  "devDependencies": {
    "@types/redux": "^3.6.0",
    "css-loader": "^6.7.3",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "ip": "^1.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "style-loader": "^1.1.2",
    "terser-webpack-plugin": "^2.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^3.7.3",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "node-static": "^0.7.11",
    "phaser": "^3.55.2",
    "redux": "^4.0.4",
    "redux-create-reducer": "^2.0.0",
    "redux-devtools-extension": "^2.13.8"
  }
}
